<table class="zebra-striped">
  <thead>
    <th>#</th>
    <th>Name</th>
    <th>Likes</th>
  </thead>
  <tbody>
  <% @companies.each do |company| %>
    <tr>
      <td><%= company.id %></td>
      <td><%= company.name %></td>
      <td><%= company.score %></td>
    </tr>
  <% end %>
  </tbody>
</table>
